{
  "version": "1.4.0",
  "homepage": "https://github.com/dobs/react-preserve-ratio",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests --testPathIgnorePatterns=cypress",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "prop-types": ">15.0.0",
    "react": ">16.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-preserve-ratio",
  "author": "Daniel O'Brien",
  "module": "dist/react-preserve-ratio.esm.js",
  "size-limit": [
    {
      "path": "dist/react-preserve-ratio.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-preserve-ratio.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/resize-observer-browser": "^0.1.6",
    "babel-loader": "^8.2.2",
    "husky": "^7.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "size-limit": "^5.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dobs/react-preserve-ratio.git"
  }
}
