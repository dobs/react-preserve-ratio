{
  "name": "react-preserve-ratio",
  "author": "Daniel O'Brien",
  "homepage": "https://github.com/dobs/react-preserve-ratio",
  "license": "Apache-2.0",
  "version": "1.4.3",
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "web-vitals": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "microbundle",
    "start": "microbundle watch",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/react-preserve-ratio.cjs",
    "default": "./dist/react-preserve-ratio.modern.js"
  },
  "main": "./dist/react-preserve-ratio.cjs",
  "module": "./dist/react-preserve-ratio.module.js",
  "unpkg": "./dist/react-preserve-ratio.umd.js",
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.0",
    "@types/react-resizable": "^1.7.4",
    "@types/resize-observer-browser": "^0.1.6",
    "microbundle": "^0.14.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-resizable": "^3.0.4",
    "react-scripts": "4.0.3",
    "typescript": "^4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dobs/react-preserve-ratio.git"
  }
}
